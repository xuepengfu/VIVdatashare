FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    INLET
    {
     type               zeroGradient;
    }

    OUTLET
    {
        type            fixedValue;
        value           uniform 0;
    }

    CYLINDER
    {
        type            zeroGradient;
    }
    
    UPSLIPWALL
    {
        type            symmetry;
    }

    DOWNSLIPWALL
    {
        type            symmetry;
    }

    UPSYM
    {
        type            cyclic;
    }

    DOWNSYM
    {
        type            cyclic;
    }

}

